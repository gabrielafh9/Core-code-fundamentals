Algoritmo TablaDemultiplicar
	Imprimir "tabla de multiplicar"
	Imprimir "Ingresar numero"
	Leer tabla
	mult <- 1
	Mientras mult <= 10 Hacer
		Imprimir "tabla: " + ConvertirATexto(tabla) " * " + ConvertirATexto(mult)
		Imprimir " = " + ConvertirATexto(tabla*mult)
		mult <- mult+1
	FinMientras
	
FinAlgoritmo
